<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.christian.flashfocus" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_menu_black_24dp"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".FlashFocusActivity"
                  android:theme = "@style/FlashFocusActionBar"  >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewCardSetActivity"
            android:label="@string/app_name"
            android:theme="@style/FlashFocusActionBar">

        </activity>
        <activity
            android:name=".EditCardActivity"
            android:theme = "@style/FlashFocusActionBar"
            >

        </activity>
        <activity android:name=".SetListActivity"
                  android:theme = "@style/FlashFocusActionBar">

        </activity>
        <activity android:name=".QuizActivity"
                  android:theme="@style/FlashFocusActionBar">

        </activity>
    </application>

</manifest>
